   "source": [
    "zeinuki = int(input(\"税抜価格を入力してください（円）: \"))\n",
    "\n",
    "zeikomi = int(zeinuki * 1.1)\n",
    "\n",
    "if zeikomi >= 2000:\n",
    "    shipping = 0\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    shipping = 350\n",
    "    print(f\"送料は{shipping}円です\")\n",
    "\n",
    "total = zeikomi + shipping\n",
    "print(f\"送料込税込価格は{total}円です\")"
   ]
  }
